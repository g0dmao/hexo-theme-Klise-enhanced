
<div class="wrapper">
  <main class="page-content">
  <header class="header">
    <h1 class="header-title center">类别：<%=page.category%></h1>
  </header>
    <% var last_year = ''; %>
    <% page.posts.each(function (article) { %>
      <article class="post-item">
        <h3 class="post-item-title">
          <a href="<%=url_for(article.path)%>">
            <%=article.title%>
          </a>
        </h3>
      </article>
    <% }) %>
  </main>
</div>
